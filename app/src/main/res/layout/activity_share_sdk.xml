<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShareSdkActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="分享"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_weixinhaoyou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="微信"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_weixinpengyouquan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="微信朋友圈"
        app:layout_constraintLeft_toRightOf="@id/share_weixinhaoyou"
        app:layout_constraintTop_toBottomOf="@id/share" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_weixinshoucang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="微信收藏"
        app:layout_constraintLeft_toRightOf="@id/share_weixinpengyouquan"
        app:layout_constraintTop_toBottomOf="@id/share" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_douyin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="抖音"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share_zhifubaohaoyou" />

       <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_google"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="谷歌登录"
        app:layout_constraintLeft_toRightOf="@+id/share_douyin"
        app:layout_constraintTop_toBottomOf="@id/share_zhifubaohaoyou" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="line登录"
        app:layout_constraintLeft_toRightOf="@+id/share_google"
        app:layout_constraintTop_toBottomOf="@id/share_zhifubaohaoyou" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_ins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="ins登录"
        app:layout_constraintLeft_toRightOf="@+id/login_line"
        app:layout_constraintTop_toBottomOf="@id/share_zhifubaohaoyou" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_qqkongjian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="qq空间"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share_weixinshoucang" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="qq"
        app:layout_constraintLeft_toRightOf="@id/share_qqkongjian"
        app:layout_constraintTop_toBottomOf="@id/share_weixinshoucang" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_weibo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="微博分享"
        app:layout_constraintLeft_toRightOf="@id/share_qq"
        app:layout_constraintTop_toBottomOf="@id/share_weixinshoucang" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_zhifubaohaoyou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="支付宝好友"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share_qq" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_zhifubaoshenghuoquan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="支付宝生活圈"
        app:layout_constraintLeft_toRightOf="@id/share_zhifubaohaoyou"
        app:layout_constraintTop_toBottomOf="@id/share_qq" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_qiyeweixin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="企业微信"
        app:layout_constraintLeft_toRightOf="@id/share_zhifubaoshenghuoquan"
        app:layout_constraintTop_toBottomOf="@id/share_qq" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/kakao_denglu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="kakao登录"
        app:layout_constraintLeft_toRightOf="@id/share_qiyeweixin"
        app:layout_constraintTop_toBottomOf="@id/share_qq" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_qqdenglu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="qq登录"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share_douyin" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_weixindenglu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="微信登录"
        app:layout_constraintLeft_toRightOf="@id/share_qqdenglu"
        app:layout_constraintTop_toBottomOf="@id/share_douyin" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_weibodenglu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="微博登录"
        app:layout_constraintLeft_toRightOf="@id/share_weixindenglu"
        app:layout_constraintTop_toBottomOf="@id/share_douyin" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_douyindenglu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="抖音登录"
        app:layout_constraintLeft_toRightOf="@id/share_weibodenglu"
        app:layout_constraintTop_toBottomOf="@id/share_douyin" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_js"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="JS插件"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share_douyindenglu" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_facebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="share_facebook"
        app:layout_constraintLeft_toRightOf="@+id/share_js"
        app:layout_constraintTop_toBottomOf="@id/share_douyindenglu" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_dingding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="share_dingding"
        app:layout_constraintLeft_toRightOf="@+id/share_facebook"
        app:layout_constraintTop_toBottomOf="@id/share_douyindenglu" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_lingying"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="领英"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/share_dingding" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_twitter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="推特"
        app:layout_constraintLeft_toRightOf="@+id/share_lingying"
        app:layout_constraintTop_toBottomOf="@id/share_dingding" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="line"
        app:layout_constraintLeft_toRightOf="@+id/share_twitter"
        app:layout_constraintTop_toBottomOf="@id/share_dingding" />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/share_ins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="ins"
        app:layout_constraintLeft_toRightOf="@+id/share_line"
        app:layout_constraintTop_toBottomOf="@id/share_dingding" />
</androidx.constraintlayout.widget.ConstraintLayout>