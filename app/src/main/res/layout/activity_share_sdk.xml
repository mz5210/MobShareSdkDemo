<?xml version="1.0" encoding="utf-8"?>
<com.google.android.flexbox.FlexboxLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    app:alignContent="flex_start"
    app:alignItems="center"
    app:flexDirection="row"
    app:flexWrap="wrap"
    app:justifyContent="flex_start"
    app:showDivider="beginning|middle"
    tools:context=".ShareSdkActivity">

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:onClick="onClick"
        android:text="一键分享" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_weixinhaoyou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="微信"

        android:textAllCaps="false" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_weixinpengyouquan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="微信朋友圈" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_weixinshoucang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="微信收藏" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_douyin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="抖音" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_google"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="谷歌" />


    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_qqkongjian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="qq空间" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_qq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="qq" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_weibo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="微博" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_FacebookMessenger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="FacebookMessenger"

        android:textAllCaps="false"/>


    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_zhifubaohaoyou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="支付宝好友"

        />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_zhifubaoshenghuoquan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="支付宝生活圈" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_qiyeweixin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="企业微信" />


    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_js"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="JS插件"

        />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_facebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="Facebook"
        android:textAllCaps="false" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_dingding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="DingDing"

        android:textAllCaps="false" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_lingying"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="领英" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_twitter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="推特" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="line" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_ins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="ins" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_kouling"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"

        android:text="生成口令" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_huanyuankouling"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="还原口令" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_whatapp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="whatApp"
        android:textAllCaps="false" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/share_kakaotalk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:onClick="onClick"
        android:text="kaKaoTalk"

        android:textAllCaps="false" />
    <Button
        style="@style/ButtonStyle"
        android:id="@+id/Telegram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:onClick="onClick"
        android:text="Telegram"

        android:textAllCaps="false" />

    <TextView
        android:id="@+id/sdk_version"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"

        android:padding="10dp"/>
</com.google.android.flexbox.FlexboxLayout>